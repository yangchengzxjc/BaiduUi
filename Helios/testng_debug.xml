<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite" parallel="tests" thread-count="2">

    <parameter name="environment" value="${environment}"/>
    <parameter name="passWord" value="${passWord}"/>

    <test verbose="10" preserve-order="true" name="debug">
        <parameter name="environment" value="stage"></parameter>
        <parameter name="phoneNumber" value="14082978666"/>
        <parameter name="passWord" value="hly123456"></parameter>

        <classes>
            <class name="com.test.api.testcase.vendor.VendorSSOTest">
                <methods>
                    <include name="ssoTest2"></include>
                </methods>
            </class>
        </classes>
    </test>

    <listeners>
        <listener class-name="com.test.listener.TestLogListener"/>
        <listener class-name="com.test.listener.TestReportListener"/>
    </listeners>
</suite>

