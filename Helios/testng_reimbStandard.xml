<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite" parallel="tests" thread-count="1">

    <parameter name="environment" value="${environment}"/>
    <parameter name="phoneNumber" value="${phoneNumber}"/>
    <parameter name="passWord" value="hly12345"/>

    <test verbose="10" preserve-order="true" name="报销标准测试">
        <classes>
            <class name="com.test.api.testcase.ReimbStand.ReimbStandardTest"/>
        </classes>
    </test>

    <test verbose="10" preserve-order="true" name="报销单提交管控测试">
        <classes>
            <class name="com.test.api.testcase.ReimbStand.ReimbSubmissionControlTest"/>
        </classes>
    </test>

    <listeners>
        <listener class-name="com.test.listener.TestLogListener"/>
        <listener class-name="com.test.listener.TestReportListener"/>
    </listeners>
</suite>

