            <stringProp name="script">import com.alibaba.fastjson.JSON;
import com.alibaba.fastjson.JSONArray;
import com.alibaba.fastjson.JSONObject;


String start_date = vars.get(&quot;now&quot;)+&quot;T16:00:00Z&quot;;

String end_date = vars.get(&quot;TWODAYLATER&quot;)+&quot;T15:59:59Z&quot;;

String json = vars.get(&quot;customFormFields&quot;);
//log.info(&quot;companyid===========&quot;+vars.get(&quot;companyid&quot;));
JSONArray customFormFields = JSON.parseArray(json);

int size = customFormFields.size();

for (int i = 0; i &lt; size; i++){
		   jj = customFormFields.getJSONObject(i);
             if (jj.get(&quot;fieldName&quot;).equals(&quot;事由&quot;))
           		jj.put(&quot;value&quot;,vars.get(&quot;bxreason&quot;));
             if (jj.get(&quot;fieldName&quot;).equals(&quot;部门&quot;))
           		jj.put(&quot;value&quot;,vars.get(&quot;departmentOID&quot;));
             if (jj.get(&quot;messageKey&quot;).equals(&quot;venMaster&quot;))
           		jj.put(&quot;value&quot;,vars.get(&quot;EXPENSE_REPORT&quot;));
             if (jj.get(&quot;messageKey&quot;).equals(&quot;payee&quot;))
           		jj.put(&quot;value&quot;,vars.get(&quot;payee&quot;));
             if (jj.get(&quot;messageKey&quot;).equals(&quot;contact_bank_account&quot;))
           		jj.put(&quot;value&quot;,vars.get(&quot;contact_bank_account&quot;));
             if (jj.get(&quot;messageKey&quot;).equals(&quot;start_date&quot;))
           		jj.put(&quot;value&quot;,start_date);
             if (jj.get(&quot;messageKey&quot;).equals(&quot;end_date&quot;))
           		jj.put(&quot;value&quot;,end_date);
            
             
}
String str =customFormFields.toString();
log.info(&quot;创建日常报销单表单数据:&quot;+str);
vars.put(&quot;NcustomFormFields&quot;,str);
</stringProp>
