            <stringProp name="script">import com.alibaba.fastjson.JSON;
import com.alibaba.fastjson.JSONArray;
import com.alibaba.fastjson.JSONObject;

String json = vars.get(&quot;bxformDefaultValues&quot;);
log.info(&quot;bxformDefaultValues===========&quot;+vars.get(&quot;bxformDefaultValues&quot;));
JSONArray customFormFields = JSON.parseArray(json);
int size = customFormFields.size();

for (int i = 0; i &lt; size; i++){
		   jj = customFormFields.getJSONObject(i);
             if (jj.get(&quot;fieldName&quot;).equals(&quot;部门&quot;))
           	 vars.put(&quot;bxformDefaultdepartmentOID&quot;,jj.get(&quot;value&quot;));
             if (jj.get(&quot;fieldName&quot;).equals(&quot;成本中心&quot;))
           	 vars.put(&quot;bxformDefaultCostCenterOID&quot;,jj.get(&quot;value&quot;));
             if (jj.get(&quot;fieldName&quot;).equals(&quot;银行卡号&quot;))
           	 vars.put(&quot;bxformDefaultBankAccount&quot;,jj.get(&quot;value&quot;));          
}</stringProp>
