                <stringProp name="script">import com.alibaba.fastjson.JSON;
import com.alibaba.fastjson.JSONArray;
import com.alibaba.fastjson.JSONObject;

String json = vars.get(&quot;apply_customFormFields&quot;);
log.info(&quot;控件信息:&quot;+json);

String fullName = vars.get(&quot;fullName&quot;);

String participantOID =vars.get(&quot;userOID&quot;);

String participant=&quot;[{\&quot;participantOID\&quot;:\&quot;&quot;+participantOID+&quot;\&quot;,\&quot;fullName\&quot;:\&quot;&quot;+fullName+&quot;\&quot;,\&quot;userOID\&quot;:\&quot;&quot;+participantOID+&quot;\&quot;}]&quot;;

log.info(&quot;participant:&quot;+participant);

JSONArray customFormFields = JSON.parseArray(json);
int size = customFormFields.size();


for (int i = 0; i &lt; size; i++){
		   jj = customFormFields.getJSONObject(i);
             if (jj.get(&quot;fieldName&quot;).equals(&quot;事由&quot;)){
           		jj.put(&quot;value&quot;,&quot;新建申请单&quot;);
             }
             if (jj.get(&quot;fieldName&quot;).equals(&quot;部门&quot;)){
             	jj.put(&quot;value&quot;,&quot;306d8760-7b3c-45ca-89d8-32924b6d1ea8&quot;);
             }
             if (jj.get(&quot;fieldName&quot;).equals(&quot;公司&quot;)){
             	jj.put(&quot;value&quot;,vars.get(&quot;default_department&quot;));
             }
             if (jj.get(&quot;fieldName&quot;).equals(&quot;成本中心&quot;)){
             	jj.put(&quot;value&quot;,vars.get(&quot;costCenterItemOID&quot;));
             }
             if (jj.get(&quot;fieldName&quot;).equals(&quot;多选值列表&quot;)){
             	jj.put(&quot;value&quot;,&quot;9010&quot;);
             }
             if (jj.get(&quot;fieldName&quot;).equals(&quot;地点&quot;)){
             	jj.put(&quot;value&quot;,&quot;北京&quot;);
             }
             if (jj.get(&quot;fieldName&quot;).equals(&quot;开始日期&quot;)){
             	jj.put(&quot;value&quot;,&quot;2019-08-06&quot;);
             }
             if (jj.get(&quot;fieldName&quot;).equals(&quot;结束日期&quot;)){
             	jj.put(&quot;value&quot;,&quot;2019-08-08&quot;);
             }
             if (jj.get(&quot;fieldName&quot;).equals(&quot;数字&quot;)){
             	jj.put(&quot;value&quot;,&quot;2019&quot;);
             }
             if (jj.get(&quot;fieldName&quot;).equals(&quot;参与人员&quot;)){
             	jj.put(&quot;value&quot;,participant);
             }
}
String str =customFormFields.toString();
log.info(&quot;NcustomFormFields:&quot;+str);
vars.put(&quot;NcustomFormFields&quot;,str);

</stringProp>
