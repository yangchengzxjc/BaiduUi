//总可用
Double expAvailableAmount = Double.parseDouble(&quot;${expAvailableAmount}&quot;);
log.info(&quot;**初始查询预算--预算额结果：**&quot;+bgtAmount);
log.info(&quot;**初始查询预算--保留额结果：**&quot;+expReserveAmount);
