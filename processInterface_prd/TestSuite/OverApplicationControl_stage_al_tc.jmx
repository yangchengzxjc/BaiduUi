import java.math.BigDecimal;
DecimalFormat df = new DecimalFormat(&quot;#.00&quot;);


String am=vars.get(&quot;apamount&quot;);
int amount0=Integer.parseInt(am)*1.2;
int amount=1+Integer.parseInt(am)*1.2;

vars.put(&quot;bxamount&quot;,String.valueOf(amount));
