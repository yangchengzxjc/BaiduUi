double p2t=Integer.parseInt(am)*1.2-Integer.parseInt(am)*0.2;

double p3t=p2t+p1t*(Float.parseFloat(vars.get(&quot;CurrencyRate&quot;)))-Integer.parseInt(am)*1.2;



BigDecimal b1 = new BigDecimal(p1t*100);
