vars.put(&quot;bxamount2&quot;,f2+&quot;&quot;);
vars.put(&quot;bxamount3&quot;,f1+&quot;&quot;);


log.info(f1+&quot;&quot;);
log.info(f2+&quot;&quot;);
log.info(f3+&quot;&quot;);





