calendar.add(calendar.DATE,0);
date = calendar.getTime();
SimpleDateFormat formatter = new SimpleDateFormat(&quot;mm:ss&quot;);
String localTime = formatter.format(date);
vars.put(&quot;NewBJEndDate&quot;,localTime);

