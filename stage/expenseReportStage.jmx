            }
            if(jsonObject.get(&quot;fieldName&quot;).getAsString().equals(&quot;费用承担公司&quot;)){
            	jsonObject.addProperty(&quot;value&quot;,&quot;740cd3b3-1097-4f20-9c0b-40edf81fecf7&quot;);
            }
            if(jsonObject.get(&quot;fieldName&quot;).getAsString().equals(&quot;开始日期&quot;)){
            	jsonObject.addProperty(&quot;value&quot;,vars.get(&quot;today&quot;));
            	jsonObject.addProperty(&quot;endDate&quot;,vars.get(&quot;afterFiveDays&quot;));
