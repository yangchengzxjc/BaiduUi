            if(jsonObject.get(&quot;fieldName&quot;).getAsString().equals(&quot;目的地&quot;)){
            	jsonObject.addProperty(&quot;value&quot;,&quot;CHN031000000&quot;);
            }
            if(jsonObject.get(&quot;fieldName&quot;).getAsString().equals(&quot;事由&quot;)){
            	jsonObject.addProperty(&quot;value&quot;,&quot;性能测试&quot;);
            }   
        }
String str =customFormFields.toString();
vars.put(&quot;custFormValues&quot;,str);

</stringProp>
